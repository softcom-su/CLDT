	<extension
		point="org.eclipse.core.expressions.definitions">
		<definition id="su.softcom.cldt.enablement">
			<and>
				<instanceof value="org.eclipse.core.resources.IProject" />
				<or>
					<adapt type="org.eclipse.core.resources.IResource">
						<test
							property="org.eclipse.core.resources.projectNature"
							value="su.softcom.cu.core.natures.c">
						</test>
					</adapt>
					<adapt type="org.eclipse.core.resources.IResource">
						<test
							property="org.eclipse.core.resources.projectNature"
							value="su.softcom.cu.core.natures.cpp">
						</test>
					</adapt>
					<adapt type="org.eclipse.core.resources.IResource">
						<test
							property="org.eclipse.core.resources.projectNature"
							value="su.softcom.cu.core.natures.ruc">
						</test>
					</adapt>
					<adapt
						type="org.eclipse.core.resources.IResource">
						<test
							property="org.eclipse.core.resources.projectNature"
							value="su.softcom.cldt.core.cnature">
						</test>
					</adapt>
				</or>
			</and>
		</definition>
	</extension>
