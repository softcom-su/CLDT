	<extension
		point="org.eclipse.ui.editors.markerAnnotationSpecification">
		<specification
			annotationType="su.softcom.cldt.ui.quickfix.error"
			colorPreferenceKey="su.softcom.cldt.ui.annotation.error.color"
			colorPreferenceValue="255,0,0"
			contributesToHeader="true"
			highlightPreferenceValue="true"
			includeOnPreferencePage="true"
			isGoToNextNavigationTarget="true"
			isGoToNextNavigationTargetKey="su.softcom.cldt.ui.annotation.error.GoToNextNavigationTarget"
			isGoToPreviousNavigationTarget="true"
			isGoToPreviousNavigationTargetKey="su.softcom.cldt.ui.annotation.error.GoToPreviousNavigationTarget"
			label="CLDT Problem Marker"
			overviewRulerPreferenceKey="su.softcom.cldt.ui.annotation.error.overview"
			overviewRulerPreferenceValue="true"
			presentationLayer="5"
			showInNextPrevDropdownToolbarAction="true"
			showInNextPrevDropdownToolbarActionKey="su.softcom.cldt.ui.annotation.error.NextPrevDropdownToolbar"
			symbolicIcon="error"
			textPreferenceKey="su.softcom.cldt.ui.annotation.error.text"
			textPreferenceValue="true"
			textStylePreferenceKey="su.softcom.cldt.ui.annotation.error.textstyle"
			textStylePreferenceValue="PROBLEM_UNDERLINE"
			verticalRulerPreferenceKey="su.softcom.cldt.ui.annotation.error.ruler"
			verticalRulerPreferenceValue="true">
		</specification>
	</extension>
