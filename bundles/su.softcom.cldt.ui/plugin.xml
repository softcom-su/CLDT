				id="su.softcom.cldt.analysisMenu"
				label="Анализ"
				mnemonic="Анализ">
				<visibleWhen
					checkEnabled="false">
					<iterate ifEmpty="False">
						<adapt
							type="org.eclipse.core.resources.IResource">
							<or>
