				style="push">
			</command>
			<command
				commandId="org.eclipse.ui.project.properties"
				label="Свойства"
				style="push">
				<visibleWhen
					checkEnabled="false">
					<iterate
						ifEmpty="false"
						operator="and">
						<or>
							<instanceof
								value="su.softcom.hsa.solution.model.api.SolutionComponent">
							</instanceof>
						</or>
					</iterate>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu">
